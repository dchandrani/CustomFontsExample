<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <font
        android:font="@font/simonetta_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/simonetta_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <font
        android:font="@font/simonetta_italic"
        android:fontStyle="italic"
        android:fontWeight="400"
        app:font="@font/simonetta_italic"
        app:fontStyle="italic"
        app:fontWeight="400" />

    <font
        android:font="@font/simonetta_black"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/simonetta_black"
        app:fontStyle="normal"
        app:fontWeight="700" />

    <font
        android:font="@font/simonetta_black_italic"
        android:fontStyle="italic"
        android:fontWeight="700"
        app:font="@font/simonetta_black_italic"
        app:fontStyle="italic"
        app:fontWeight="700" />
</font-family>